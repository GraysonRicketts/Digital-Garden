{
  "private": true,
  "name": "remix-app-template",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --minify",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "format:check": "yarn prettier:check && yarn lint:check",
    "format:fix": "yarn prettier:fix && yarn lint:fix",
    "prettier:check": "yarn prettier --check 'app/**/**.ts'",
    "prettier:fix": "yarn prettier --write 'app/**/**.ts'",
    "lint:check": "yarn eslint app/**/**.ts",
    "lint:fix": "yarn eslint --fix app/**/**.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@types/marked": "^4.0.2",
    "classnames": "^2.3.1",
    "config": "^3.3.7",
    "front-matter": "^4.0.2",
    "marked": "^4.0.12",
    "prisma": "^3.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/config": "^0.0.41",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "16.13"
  },
  "engineStrict": true,
  "sideEffects": false
}
